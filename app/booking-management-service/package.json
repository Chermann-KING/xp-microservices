{
    "name": "booking-management-service",
    "version": "1.0.0",
    "description": "Booking Management Microservice for Tourism Booking Application",
    "main": "server.js",
    "type": "module",
    "scripts": {
        "start": "node server.js",
        "dev": "nodemon server.js",
        "db:migrate": "node src/database/migrate.js",
        "db:seed": "node src/database/seed.js",
        "db:reset": "npm run db:migrate && npm run db:seed"
    },
    "keywords": [
        "microservice",
        "booking",
        "reservation",
        "rest-api",
        "postgresql",
        "sequelize"
    ],
    "author": "Tourism App Team",
    "license": "MIT",
    "dependencies": {
        "amqplib": "^0.10.4",
        "axios": "^1.7.9",
        "cors": "^2.8.5",
        "dotenv": "^16.4.7",
        "express": "^4.21.1",
        "pg": "^8.16.3",
        "pg-hstore": "^2.3.4",
        "sequelize": "^6.37.7",
        "uuid": "^11.0.3"
    },
    "devDependencies": {
        "nodemon": "^3.1.9"
    },
    "engines": {
        "node": ">=20.0.0"
    }
}